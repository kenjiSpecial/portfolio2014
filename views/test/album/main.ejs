<% include ../header.ejs %>

<div id="admin-album-main-container" class="container main-container">

    <div id="admin-album-upload-row" class="row">

        <form method="post" action="/test-upload" enctype="multipart/form-data">
            <div class="form-group">
                <label >File input</label>
                <input type="file" name="image">

            </div>
            <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div>

    <div class="row" id="admin-album-view-row">
        <%for(var i = 0;i < docs.length; i++){
            var image = docs[i].name;
        %>
            <div class="col-xs-6 col-md-3">
                <div class="thumbnail">
                    <img src="../images/test/<%= image %>" alt="...">
                </div>
            </div>

        <%}%>


    </div>

</div>

<% include ../footer.ejs %>